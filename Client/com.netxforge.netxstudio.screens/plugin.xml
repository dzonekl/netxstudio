<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            category="com.netxforge.netxstudio.ui"
            class="com.netxforge.netxstudio.screens.demo.DemoViewPartSelector"
            id="com.netxforge.netxstudio.screens.demo.DemoViewPartSelector"
            name="Demo">
      </view>
            <view
                  category="com.netxforge.netxstudio.ui"
                  class="com.netxforge.netxstudio.screens.selector.ScreenSelector"
                  id="com.netxforge.netxstudio.screens.selector.demo"
                  name="ScreenSelector">
      </view>
     <view
                  category="com.netxforge.netxstudio.ui"
                  class="com.netxforge.netxstudio.screens.selector.AdminScreenSelector"
                  id="com.netxforge.netxstudio.screens.selector.admin"
                  name="AdminSelector">
      </view>
     <view
                  category="com.netxforge.netxstudio.ui"
                  class="com.netxforge.netxstudio.screens.selector.MonitoringScreenSelector"
                  id="com.netxforge.netxstudio.screens.selector.monitoring"
                  name="MonitoringSelector">
      </view>
     <view
                  category="com.netxforge.netxstudio.ui"
                  class="com.netxforge.netxstudio.screens.selector.LibraryScreenSelector"
                  id="com.netxforge.netxstudio.screens.selector.library"
                  name="LibrarySelector">
      </view>
      
      <!-- Define Viewparts for various selectors. -->
      
   </extension>
   
   <extension
         point="org.eclipse.ui.editors">
      <editor
            name="ScreensEditor"
            default="true"
            class="com.netxforge.netxstudio.screens.demo.ScreensEditor"
            id="com.netxforge.netxstudio.screens.demo.editor">
      </editor>
   </extension>
   
</plugin>
