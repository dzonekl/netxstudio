# NetXStudio Server tracing options 
# Enables debugging and/or tracing for server side services. 
com.netxforge.netxstudio.data/debug=true
com.netxforge.nextstudio.common/debug=true
com.netxforge.netxstudio.screens/debug=true
com.netxforge.netxstudio.screens.editing/debug=false

# Screens facilities
com.netxforge.netxstudio.screens/trace.screens=true;
com.netxforge.netxstudio.screens/trace.screens.binding=true


# Editing facilities
com.netxforge.netxstudio.screens.editing/trace.editing=true
com.netxforge.netxstudio.screens.editing/trace.editing.details=true

# Data facilities
com.netxforge.netxstudio.data/trace.data=true
com.netxforge.netxstudio.data/trace.data.details=true

#CDO Tracing Options. 
org.eclipse.emf.cdo/debug = true

org.eclipse.emf.cdo/debug.session = true
org.eclipse.emf.cdo/debug.session.model = true
org.eclipse.emf.cdo/debug.session.revision = true

org.eclipse.emf.cdo/debug.view = true
org.eclipse.emf.cdo/debug.view.transaction = true
org.eclipse.emf.cdo/debug.view.audit = false

org.eclipse.emf.cdo/debug.object = false
org.eclipse.emf.cdo/debug.object.statemachine = false
org.eclipse.emf.cdo/debug.object.store = false

org.eclipse.emf.cdo/load.resource.before.notification = false