# NetXstudio Server tracing options 
# Enables debugging and/or tracing for server side services.
com.netxforge.netxscript/debug=true 
com.netxforge.netxstudio.server/debug=false
com.netxforge.netxstudio.server.metrics/debug=false
com.netxforge.netxstudio.server.logic/debug=true
com.netxforge.netxstudio.server.job/debug=false
# Enables debugging for common Appl plugins
com.netxforge.netxstudio.common/debug=false
com.netxforge.netxstudio.data/debug=true


# NetXScript trace options
com.netxforge.netxscript/trace.netxscript=true
com.netxforge.netxscript/trace.netxscript.scoping=true
com.netxforge.netxscript/trace.netxscript.expression=true
com.netxforge.netxscript/trace.netxscript.description=true

#Data CDO trace options.
com.netxforge.netxstudio.data/trace.data=false
com.netxforge.netxstudio.data/trace.data.details=false
com.netxforge.netxstudio.data/trace.data.onfe=false

# Data Import trace options. (As the importers are in the .data plugin, options as well). 
com.netxforge.netxstudio.data/trace.import=true
com.netxforge.netxstudio.data/trace.import.details=true
com.netxforge.netxstudio.data/trace.import.locator=true
com.netxforge.netxstudio.data/trace.import.helper=true

#Data Result processing options
com.netxforge.netxstudio.data/trace.result.value=true
com.netxforge.netxstudio.data/trace.result.expression=true
com.netxforge.netxstudio.data/trace.result.tolerance=true

#Data Component indexing option
com.netxforge.netxstudio.data/trace.component.index=true

# Common trace options
com.netxforge.netxstudio.common/trace.utils=true
com.netxforge.netxstudio.common/trace.common.properties=true

# Server trace options
com.netxforge.netxstudio.server/trace.server.commitinfo=true
com.netxforge.netxstudio.server/trace.server.cdo=true 

# Jobs trace options.
com.netxforge.netxstudio.server.job/trace.jobs=true

# Metrics trace options. 
com.netxforge.netxstudio.server.metrics/trace.import=true

# Logic trace options.
com.netxforge.netxstudio.server.logic/trace.logic=true
com.netxforge.netxstudio.server.logic/trace.logic.details=true

# Logic reporting options.
com.netxforge.netxstudio.server.logic/trace.report=true
com.netxforge.netxstudio.server.logic/trace.report.details=true

# Logic profiles options.
com.netxforge.netxstudio.server.logic/trace.profile=true
com.netxforge.netxstudio.server.logic/trace.profile.details=true

# Logic retention options. 
com.netxforge.netxstudio.server.logic/trace.retention=true
com.netxforge.netxstudio.server.logic/trace.retention.details=true

# Logic expression options. 
com.netxforge.netxstudio.server.logic/trace.expression=true
com.netxforge.netxstudio.server.logic/trace.expression.details=true

# Enable debugging for CDO plugins
org.eclipse.net4j.util/debug=false
org.eclipse.emf.cdo/debug=false
org.eclipse.emf.cdo/debug.session=false

org.eclipse.emf.cdo.server.db/debug=true

# CDO Server
org.eclipse.emf.cdo.server/debug=false
org.eclipse.emf.cdo.server/debug.repository=false
org.eclipse.emf.cdo.server/debug.resource=false
org.eclipse.emf.cdo.server/debug.revision=false
org.eclipse.emf.cdo.server/debug.session=false
org.eclipse.emf.cdo.server/debug.store=false
org.eclipse.emf.cdo.server/debug.protocol=false
org.eclipse.emf.cdo.server/debug.transaction=false
org.eclipse.emf.cdo.server/debug.types=false

org.eclipse.emf.cdo.server.net4j/debug=false

# CDO NET4J

org.eclipse.emf.cdo.net4j/debug=false
org.eclipse.emf.cdo.net4j/debug.protocol=false

# NET4J DB
org.eclipse.net4j.db/debug=false
org.eclipse.net4j.db/debug.sql=false

# NET4J
org.eclipse.net4j/debug=true
org.eclipse.net4j/debug.acceptor=false
org.eclipse.net4j/debug.connector=false
org.eclipse.net4j/debug.channel=false
org.eclipse.net4j/debug.buffer=false
org.eclipse.net4j/debug.buffer.stream=false

# NET4J DB.MYSQL
org.eclipse.net4j.db.mysql/debug=false
org.eclipse.net4j.db.mysql/debug.sql=false
