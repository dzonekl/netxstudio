<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="id1"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="com.netxforge.oss2.product.OSS2Application">
         </run>
      </application>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="com.netxforge.oss2.product.id1"
            name="oss2app">
         <property
               name="appName"
               value="oss2app">
         </property>
      </product>
   </extension>

</plugin>
