<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="com.netxforge.netxscript.rc1.rcp.Application">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            name="Perspective"
            class="com.netxforge.netxscript.rc1.rcp.Perspective"
            id="com.netxforge.netxscript.rc1.rcp.perspective">
      </perspective>
   </extension>
   
